<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">


    <servlet>
        <servlet-name>textsearch</servlet-name>
        <servlet-class>com.google.appengine.demos.search.TextSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>textsearch</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>arabiansearch</servlet-name>
        <servlet-class>com.google.appengine.demos.search.ArabianSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>arabiansearch</servlet-name>
        <url-pattern>/nights</url-pattern>
    </servlet-mapping>
    

    <welcome-file-list>
      <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>




	<servlet>
	    <servlet-name>internals</servlet-name>
	    <servlet-class>com.mru.cloud.InternalsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
	    <servlet-name>internals</servlet-name>
	    <url-pattern>/internals/*</url-pattern>
	</servlet-mapping>	  

</web-app>
