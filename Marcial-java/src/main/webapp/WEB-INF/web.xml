<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">


    <servlet>
        <servlet-name>user</servlet-name>
        <servlet-class>com.google.appengine.demos.search.USer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>asbadaspossible</servlet-name>
        <servlet-class>com.google.appengine.demos.search.AsBadAsPossible</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>asbadaspossible</servlet-name>
        <url-pattern>/asbadaspossible</url-pattern>
    </servlet-mapping>
    

    <welcome-file-list>
      <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>


</web-app>
